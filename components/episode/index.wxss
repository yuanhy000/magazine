.container {
    height: 60rpx;
    display: inline-flex;
    flex-direction: row;
}

.index-container {
    display: flex;
    flex-direction: row;
    align-items: baseline;
}

.plain {
    font-size: 34rpx;
}

.index {
    font-size: 60rpx;
    line-height: 60rpx;
    font-weight: 600;
    margin-right: 14rpx;
    font-style: "Times New Roman";
}

.line {
    height: 48rpx;
    margin-right: 14rpx;
    border-left: 1px solid #333;
}

.date-container {
    display: flex;
    flex-direction: column;
    margin-top: 5rpx;
}

.month {
    font-size: 24rpx;
    line-height: 24rpx
}

.year {
    font-size: 20rpx;
}